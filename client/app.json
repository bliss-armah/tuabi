{
  "expo": {
    "name": "tuabi",
    "slug": "tuabi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main2.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/main2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/main2.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
